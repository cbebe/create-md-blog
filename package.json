    "blog",
    "cli"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.35",
    "@types/prompts": "^2.0.14",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "front-matter": "^4.0.2",
    "js-yaml": "^4.1.0",
    "prompts": "^2.4.2",
    "readdirp": "^3.6.0",
    "slug": "^5.3.0"
